server:
  port: 0
spring:
  application:
    name: fountation-account-service
  main:
    web-application-type: reactive
  security:
    oauth2:
      resourceserver:
        jwt:
          jwk-set-uri: lb://fountation-authorization-service/oauth2/jwks
eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:8761/eureka/ # Adjust Eureka server address if needed
    fetchRegistry: true
    registerWithEureka: false # Gateway does not need to register itself
logging:
  level:
    org:
      springframework:
        security:
          oauth2: TRACE